          }
        }
      }
      SEQUENCE {  # Extension
        # cRLDistributionPoints
        OBJECT_IDENTIFIER { 2.5.29.31 }
        OCTET_STRING {
          SEQUENCE {
            SEQUENCE {
              [0] {
                [0] {
                  [6 PRIMITIVE] { "http://g.symcb.com/crls/gtglobal.crl" }
                }
              }
            }
