        # authorityKeyIdentifier
        OBJECT_IDENTIFIER { 2.5.29.35 }
        OCTET_STRING {
          SEQUENCE {
            [0 PRIMITIVE] { `01020304` }  # KeyIdentifier
          }
        }
      }
      SEQUENCE {  # Extension
        # keyUsage
        OBJECT_IDENTIFIER { 2.5.29.15 }
        BOOLEAN { `ff` }
        OCTET_STRING {
          BIT_STRING { `01ff` }
        }
      }
      SEQUENCE {  # Extension
        # authorityInfoAccess
        OBJECT_IDENTIFIER { 1.3.6.1.5.5.7.1.1 }
        OCTET_STRING {
          SEQUENCE {
            SEQUENCE {
              # ocsp
              OBJECT_IDENTIFIER { 1.3.6.1.5.5.7.48.1 }
              [6 PRIMITIVE] { "http://g.symcd.com" }
            }
          }
        }
      }
      SEQUENCE {  # Extension
        # basicConstraints
        OBJECT_IDENTIFIER { 2.5.29.19 }
        BOOLEAN { `ff` }
        OCTET_STRING {
          SEQUENCE {
            BOOLEAN { `ff` }
